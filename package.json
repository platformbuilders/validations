{
  "name": "@platformbuilders/validations",
  "version": "0.0.18",
  "description": "",
  "author": "Platform Builders <apps@platformbuilders.io>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/platformbuilders/validations.git"
  },
  "keywords": [
    "platformbuilders",
    "validations"
  ],
  "license": "ISC",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "scripts": {
    "test": "jest --passWithNoTests",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rm -rf dist && rollup -c && yarn uglify",
    "start": "rollup -c -w",
    "prepare": "husky install && yarn build",
    "pretty": "prettier --config ./.prettierrc.js --write \"src/**/*.{jsx,ts,tsx,json}\"",
    "uglify": "for f in dist/*.js; do short=${f%.js}; uglifyjs $f > $short.min.js; rm -rf $f; mv $short.min.js $short.js; done",
    "lint": "tsc && eslint '*/**/*.{ts, tsx}' --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.ts?(x)": [
      "yarn pretty",
      "yarn lint",
      "yarn test",
      "git add ."
    ]
  },
  "peerDependencies": {
    "@fnando/cnpj": "^1.0.2",
    "@fnando/cpf": "^1.0.2",
    "@platformbuilders/helpers": "^0.3.5",
    "react": "^16.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/runtime": "^7.15.4",
    "@fnando/cnpj": "^1.0.2",
    "@fnando/cpf": "^1.0.2",
    "@platformbuilders/helpers": "^0.3.6",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-url": "^6.1.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.30",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "eslint-plugin-typescript": "^0.14.0",
    "gh-pages": "^3.1.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "react": "^17.0.1",
    "react-scripts": "^4.0.1",
    "rollup": "^2.58.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.4.2",
    "typescript": "^4.4.4",
    "uglify-js": "^3.14.2"
  },
  "dependencies": {
    "dayjs": "^1.10.7"
  }
}
